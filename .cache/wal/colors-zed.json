{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#110a1990",
                "editor.background": "#110a1990",
                "editor.foreground": "#c3c1c5",
                "text": "#c3c1c5",
                "text.muted": "#c3c1c570",
                "text.ignored": "#c3c1c540",
                "text.placeholder": "#c3c1c550",
                "ignored": "#c3c1c530",
                "element.hover": "#c3c1c530",
                "ghost_element.hover": "#9f4e2130",
                "ghost_element.selected": "#9f4e2130",
                "ghost_element.active": "#9f4e2160",
                "border": "#110a19",
                "editor.highlighted_line.background": "#9f4e2110",
                "editor.active_line.background": "#9f4e2110",
                "panel.background": "#110a1990",
                "title_bar.background": "#110a1990",
                "title_bar.inactive_background": "#110a1990",
                "status_bar.background": "#110a1990",
                "drop_target.background": "#110a1990",
                "elevated_surface.background": "#110a19",
                "toolbar.background": "#110a1990",
                "tab_bar.background": "#110a1990",
                "tab.inactive_background": "#110a1990",
                "tab.active_background": "#9f4e2130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#110a19",
                "scrollbar.thumb.background": "#9f4e21",
                "editor.gutter.background": "#110a1990",
                "terminal.background": "#110a1910",
                "terminal.foreground": "#c3c1c5",
                "terminal.dim_foreground": "#c3c1c5",
                "terminal.bright_foreground": "#c3c1c5",
                "terminal.ansi.black": "#110a19",
                "terminal.ansi.red": "#9f4e21",
                "terminal.ansi.green": "#764243",
                "terminal.ansi.yellow": "#99553f",
                "terminal.ansi.blue": "#b0632c",
                "terminal.ansi.magenta": "#ad6a47",
                "terminal.ansi.cyan": "#6f5168",
                "terminal.ansi.white": "#928e98",
                "terminal.ansi.bright_black": "#63596e",
                "terminal.ansi.bright_red": "#D5682D",
                "terminal.ansi.bright_green": "#9E585A",
                "terminal.ansi.bright_yellow": "#CC7255",
                "terminal.ansi.bright_blue": "#EB843B",
                "terminal.ansi.bright_magenta": "#E78E5F",
                "terminal.ansi.bright_cyan": "#946D8B",
                "terminal.ansi.bright_white": "#c3c1c5",
                "modified": "#9f4e21",
                "syntax": {
                    "attribute": {
                        "color": "#9f4e21"
                    },
                    "boolean": {
                      "color": "#764243"
                    },
                    "comment": {
                      "color": "#63596e"
                    },
                    "comment.doc": {
                      "color": "#63596e"
                    },
                    "constant": {
                      "color": "#9E585A"
                    },
                    "function": {
                      "color": "#946D8B"
                    },
                    "keyword": {
                      "color": "#b0632c"
                    },
                    "number": {
                      "color": "#ad6a47"
                    },
                    "operator": {
                      "color": "#b0632c"
                    },
                    "string": {
                      "color": "#9f4e21"
                    },
                    "variable": {
                      "color": "#764243"
                    }
                },
                "players": [
                  {
                    "cursor": "#9f4e21",
                    "background": "#110a19",
                    "selection": "#9f4e2130"
                  }
                ]
            }
        }
    ]
}
